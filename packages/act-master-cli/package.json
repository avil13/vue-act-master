{
  "name": "act-master-cli",
  "version": "1.0.2",
  "author": "avil13",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "bin"
  ],
  "bin": {
    "act-master-cli": "bin/act-master-cli"
  },
  "engines": {
    "node": ">=16"
  },
  "homepage": "https://avil13.github.io/vue-act-master/",
  "keywords": [
    "actions",
    "events",
    "vue",
    "commander",
    "business logic",
    "cli"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "ts-node --project tsconfig.json src/index.ts",
    "build": "rm -rf ./dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.mjs.json",
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "commit": "cz",
    "lint:fix": "eslint --fix --ext .ts .",
    "test:watch": "vitest dev",
    "test": "vitest run",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.prod.json --noEmit",
    "prepublishOnly": "npm run test && npm run build && auto-changelog && git update-index"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avil13/vue-act-master.git"
  },
  "devDependencies": {
    "@types/node": "^20.1.7",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "find-up": "^5.0.0",
    "ts-morph": "^18.0.0",
    "yaml": "^2.2.2"
  }
}
