    "dev": "npm run build && npm run serve",
    "lint:fix": "eslint --fix --ext .ts .",
    "__prepublish": "npm run test && npm run build",
    "serve": "node example/server.js",
    "start": "nodemon",
    "test:watch": "jest --watch",
    "test": "jest",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p . --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-canvas-mock": "^2.2.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2",
    "vue": "^2.*",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && git update-index --again",
      "pre-push": "npm test"
    }
  }
