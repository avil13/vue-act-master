{
  "name": "vue-act-master",
  "version": "2.3.8",
  "description": "",
  "author": "avil13",
  "main": "dist/vue-act-master.umd.cjs",
  "module": "dist/vue-act-master.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/vue-act-master.umd.cjs",
      "import": "./dist/vue-act-master.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "nuxt"
  ],
  "homepage": "https://avil13.github.io/vue-act-master/",
  "keywords": [
    "actions",
    "events",
    "vue",
    "commander",
    "business logic"
  ],
  "scripts": {
    "build": "rm -rf ./dist && vite build && tsc -p tsconfig.prod.json",
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "size": "size-limit",
    "lint:fix": "eslint --fix --ext .ts .",
    "test": "vitest run",
    "test:watch": "vitest dev",
    "test:coverage": "vitest run --coverage",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.json --noEmit",
    "prepublishOnly": "npm test && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run changelog",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avil13/vue-act-master.git"
  },
  "license": "MIT",
  "dependencies": {
    "act-master": "^2.3.8"
  },
  "peerDependencies": {
    "act-master": "*"
  },
  "devDependencies": {
    "@babel/types": "^7.23.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vue": "*"
  }
}
