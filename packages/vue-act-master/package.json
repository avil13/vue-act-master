{
  "name": "vue-act-master",
  "version": "2.0.0",
  "description": "",
  "author": "avil13",
  "main": "dist/index.common.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "typings": "dist/vue-act-master/src/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "src",
    "nuxt"
  ],
  "homepage": "https://avil13.github.io/vue-act-master/",
  "keywords": [
    "actions",
    "events",
    "vue",
    "commander",
    "business logic"
  ],
  "scripts": {
    "build": "rm -rf ./dist && rollup -c",
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "lint:fix": "eslint --fix --ext .ts .",
    "test:watch": "vitest dev",
    "test": "vitest run --coverage",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.prod.json --noEmit",
    "prepublishOnly": "npm run test && npm run build && npm run changelog"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run changelog",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avil13/vue-act-master.git"
  },
  "license": "MIT",
  "dependencies": {
    "act-master": "^2.0.0"
  },
  "peerDependencies": {
    "act-master": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.1.7",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "rollup": "^2.57.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^5.0.4",
    "vue": "*"
  }
}
