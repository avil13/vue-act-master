{
  "name": "act-master",
  "version": "1.9.0",
  "author": "avil13",
  "main": "dist/index.common.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "src"
  ],
  "homepage": "https://avil13.github.io/vue-act-master/",
  "keywords": [
    "actions",
    "events",
    "vue",
    "commander",
    "business logic"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn test && rm -rf ./dist && rollup -c",
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "commit": "cz",
    "lint:fix": "eslint --fix --ext .ts .",
    "test:watch": "jest --watch",
    "test": "jest --collect-coverage --runInBand",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.prod.json --noEmit",
    "prepublishOnly": "yarn run test && yarn run build"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "yarn run changelog && git update-index --again"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avil13/vue-act-master.git"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@types/node": "^16.10.1",
    "auto-changelog": "^2.3.0",
    "babel-jest": "^27.2.2",
    "husky": "^4.3.0",
    "jest": "^27.2.2",
    "jest-canvas-mock": "^2.3.1",
    "rollup": "^2.57.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5"
  }
}
