  "description": "",
  "author": "avil13",
  "main": "dist/index.common.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "typings": "dist/vue-act-master/src/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "src",
    "nuxt"
  ],
  "homepage": "https://avil13.github.io/vue-act-master/",
  "keywords": [
    "actions",
    "events",
    "vue",
    "commander",
    "business logic"
  ],
  "scripts": {
    "build": "yarn test && rm -rf ./dist && rollup -c",
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "lint:fix": "eslint --fix --ext .ts .",
    "test:watch": "jest --watch",
    "test": "jest --collect-coverage --runInBand",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.prod.json --noEmit",
    "prepublishOnly": "npm run test && npm run build && npm run changelog"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run changelog",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avil13/vue-act-master.git"
  },
  "license": "MIT",
  "dependencies": {
    "act-master": "^1.5.*",
    "vue": "2.*"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "auto-changelog": "^2.2.1",
    "babel-jest": "^26.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-canvas-mock": "^2.3.0",
    "prettier": "^2.1.2",
    "rollup": "^2.32.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.28.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2"
