{
  "name": "vue-act-master",
  "version": "0.3.0",
  "description": "",
  "author": "avil13",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "src",
    "nuxt"
  ],
  "scripts": {
    "build": "yarn test && rm -rf ./dist && tsc -p ./tsconfig.prod.json",
    "changelog": "auto-changelog",
    "lint:fix": "eslint --fix --ext .ts .",
    "test:watch": "jest --watch",
    "test": "jest --collect-coverage",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.prod.json --noEmit",
    "_prepublish": "npm run test && npm run build && auto-changelog"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && git update-index --again && npm run changelog",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:avil13/vue-act-master.git"
  },
  "license": "MIT",
  "dependencies": {
    "vue": "^2.*"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "auto-changelog": "^2.2.1",
    "babel-jest": "^26.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-canvas-mock": "^2.3.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
