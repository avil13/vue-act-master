{
  "name": "act-master-cli",
  "version": "0.2.0",
  "author": "avil13",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "src",
    "bin"
  ],
  "bin": {
    "act-master-cli": "bin/act-master-cli"
  },
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://avil13.github.io/vue-act-master/",
  "keywords": [
    "actions",
    "events",
    "vue",
    "commander",
    "business logic",
    "cli"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "ts-node --project tsconfig.json src/index.ts",
    "build": "yarn test && rm -rf ./dist && tsc --project tsconfig.prod.json",
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "commit": "cz",
    "lint:fix": "eslint --fix --ext .ts .",
    "test:watch": "vitest dev",
    "test": "vitest run --coverage",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.prod.json --noEmit",
    "prepublishOnly": "npm run test && npm run build && auto-changelog && git update-index"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "npm run changelog && git update-index --again"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avil13/vue-act-master.git"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "auto-changelog": "^2.2.1",
    "husky": "^4.3.0",
    "rollup": "^2.32.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.28.0",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "ajv": "^7.2.3",
    "find-up": "^5.0.0",
    "ts-morph": "^10.0.2"
  }
}
