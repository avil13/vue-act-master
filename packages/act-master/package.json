{
  "name": "act-master",
  "version": "1.4.1",
  "author": "avil13",
  "main": "dist/index.common.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "typings": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "src"
  ],
  "homepage": "https://avil13.github.io/vue-act-master/",
  "keywords": [
    "actions",
    "events",
    "vue",
    "commander",
    "business logic"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn test && rm -rf ./dist && rollup -c",
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "commit": "cz",
    "lint:fix": "eslint --fix --ext .ts .",
    "test:watch": "jest --watch",
    "test": "jest --collect-coverage --runInBand",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.prod.json --noEmit",
    "prepublish": "npm run test && npm run build && auto-changelog && git update-index"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "npm run changelog && git update-index --again"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avil13/vue-act-master.git"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "auto-changelog": "^2.2.1",
    "babel-jest": "^26.6.0",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "jest-canvas-mock": "^2.3.0",
    "rollup": "^2.32.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.28.0",
    "ts-jest": "^26.4.1"
  }
}
