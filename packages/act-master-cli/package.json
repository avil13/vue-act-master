{
  "name": "act-master-cli",
  "version": "1.3.2",
  "author": "avil13",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.cjs"
    }
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "bin"
  ],
  "bin": {
    "act-master-cli": "bin/act-master-cli",
    "act-cli": "bin/act-master-cli"
  },
  "engines": {
    "node": ">=20"
  },
  "homepage": "https://avil13.github.io/vue-act-master/",
  "keywords": [
    "actions",
    "events",
    "vue",
    "commander",
    "business logic",
    "cli"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev:init": "tsx src/index.ts init",
    "dev:g": "tsx src/index.ts g",
    "build": "tsdown",
    "lint:fix": "eslint --fix --ext .ts .",
    "test:watch": "vitest",
    "test": "vitest run",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.prod.json --noEmit",
    "prepublishOnly": "npm run test && npm run build && git update-index"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avil13/vue-act-master.git"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ajv": "^8.17.1",
    "find-up": "8.0.0",
    "ts-morph": "^27.0.2",
    "tsdown": "^0.17.0",
    "tsx": "^4.21.0",
    "vitest": "^4.0.15",
    "yaml": "^2.8.2"
  }
}