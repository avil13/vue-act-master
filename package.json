{
  "name": "vue-act-master",
  "version": "0.0.7",
  "description": "",
  "author": "avil13",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "src",
    "nuxt"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc -p .",
    "dev:watch": "concurrently --kill-others 'nodemon' 'npm run serve'",
    "dev": "npm run build && npm run serve",
    "lint:fix": "eslint --fix --ext .ts .",
    "serve": "webpack-dev-server --config ./example/webpack.config.js",
    "start": "npm run dev",
    "test:watch": "jest --watch",
    "test": "jest",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p . --noEmit",
    "prepublish": "npm run test && npm run build"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-canvas-mock": "^2.2.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2",
    "vue": "^2.*",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && git update-index --again",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:avil13/vue-act-master.git"
  },
  "license": "MIT"
}
