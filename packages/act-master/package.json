{
  "name": "act-master",
  "version": "1.12.0",
  "author": "avil13",
  "main": "dist/act-master.umd.js",
  "module": "dist/act-master.mjs",
  "typings": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/act-master.umd.js",
      "import": "./dist/act-master.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "README.md",
    "dist",
    "src"
  ],
  "homepage": "https://avil13.github.io/vue-act-master/",
  "keywords": [
    "actions",
    "events",
    "vue",
    "commander",
    "business logic"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist && vite build && tsc -p tsconfig.prod.json",
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "size": "size-limit",
    "lint:fix": "eslint --fix --ext .ts .",
    "test": "vitest run",
    "test:watch": "vitest dev",
    "test:coverage": "vitest run --coverage",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.json --noEmit",
    "prepublishOnly": "yarn test && yarn run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run changelog && git update-index --again"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avil13/vue-act-master.git"
  },
  "devDependencies": {
    "@types/node": "^18.14.5",
    "auto-changelog": "^2.4.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  }
}
