{
  "name": "vue-act-master",
  "version": "0.2.0",
  "description": "",
  "author": "avil13",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "src",
    "nuxt"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc -p ./tsconfig.prod.json",
    "lint:fix": "eslint --fix --ext .ts .",
    "test:watch": "jest --watch",
    "test": "jest --collect-coverage",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.prod.json --noEmit",
    "prepublish": "npm run test && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && git update-index --again",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:avil13/vue-act-master.git"
  },
  "license": "MIT",
  "dependencies": {
    "vue": "^2.*"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "jest-canvas-mock": "^2.2.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
