{
  "name": "act-master",
  "version": "1.10.8",
  "author": "avil13",
  "main": "dist/index.common.min.js",
  "module": "dist/index.esm.min.js",
  "browser": "dist/index.umd.min.js",
  "typings": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "src"
  ],
  "homepage": "https://avil13.github.io/vue-act-master/",
  "keywords": [
    "actions",
    "events",
    "vue",
    "commander",
    "business logic"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn test && rm -rf ./dist && rollup -c",
    "changelog": "auto-changelog && git add CHANGELOG.md",
    "size": "size-limit",
    "lint:fix": "eslint --fix --ext .ts .",
    "test:watch": "vitest dev",
    "test": "vitest run --coverage",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check": "tsc -p ./tsconfig.json --noEmit",
    "prepublishOnly": "yarn run test && yarn run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run changelog && git update-index --again"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avil13/vue-act-master.git"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@size-limit/time": "^7.0.8",
    "@types/node": "^17.0.21",
    "auto-changelog": "^2.4.0",
    "rollup": "^2.69.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "size-limit": "^7.0.8"
  }
}
